{
  "env": {
      "es6": true,
      "node": true
  },
  "plugins": ["jest", "prettier"],
  "extends": ["airbnb-base", "plugin:jest/recommended", "plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "strict": 0,
    "linebreak-style": 0,
    "indent": ["error", 2]
  }
}
