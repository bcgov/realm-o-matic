    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "email-templates": "^5.1.0",
    "express": "^4.17.1",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "jsonata": "^1.8.3",
    "lodash": "^4.17.21",
    "nconf": "^0.10.0",
